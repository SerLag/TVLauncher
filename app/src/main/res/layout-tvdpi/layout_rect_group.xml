<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_rect_group"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_video"
        android:layout_width="240dp"
        android:layout_height="135dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="90dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="beforeDescendants">
        <ImageView
            android:id="@+id/img_video"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"/>
    </com.droidlogic.tvlauncher.MyRelativeLayout>

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_youtube"
        android:layout_width="240dp"
        android:layout_height="135dp"
        android:layout_marginLeft="306dp"
        android:layout_marginTop="90dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="beforeDescendants">
        <ImageView
            android:id="@+id/img_youtube"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"/>
    </com.droidlogic.tvlauncher.MyRelativeLayout>

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_kodi"
        android:layout_width="240dp"
        android:layout_height="135dp"
        android:layout_marginLeft="561dp"
        android:layout_marginTop="90dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="beforeDescendants">
        <ImageView
            android:id="@+id/img_kodi"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"/>
    </com.droidlogic.tvlauncher.MyRelativeLayout>

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_browser"
        android:layout_width="240dp"
        android:layout_height="135dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="240dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="beforeDescendants">

        <ImageView
            android:id="@+id/img_browser"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"/>
    </com.droidlogic.tvlauncher.MyRelativeLayout>

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_playstore"
        android:layout_width="240dp"
        android:layout_height="135dp"
        android:layout_marginLeft="306dp"
        android:layout_marginTop="240dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="beforeDescendants">
        <ImageView
            android:id="@+id/img_playstore"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true" />
    </com.droidlogic.tvlauncher.MyRelativeLayout>

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_miracast"
        android:layout_width="240dp"
        android:layout_height="135dp"
        android:layout_marginLeft="561dp"
        android:layout_marginTop="240dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:descendantFocusability="beforeDescendants">

        <ImageView
            android:id="@+id/img_miracast"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
/>
    </com.droidlogic.tvlauncher.MyRelativeLayout>

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_setting"
        android:layout_width="87dp"
        android:layout_height="87dp"
        android:layout_marginLeft="817dp"
        android:layout_marginTop="90dp"
        android:descendantFocusability="beforeDescendants"
        android:focusable="true"
        android:focusableInTouchMode="true">

        <ImageView
            android:id="@+id/img_setting"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:layout_margin="3dp"
            android:padding="3dp"
            android:scaleType="centerCrop" />
    </com.droidlogic.tvlauncher.MyRelativeLayout>

    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_filemanager"
        android:layout_width="87dp"
        android:layout_height="87dp"
        android:layout_marginLeft="817dp"
        android:layout_marginTop="189dp"
        android:descendantFocusability="beforeDescendants"
        android:focusable="true"
        android:focusableInTouchMode="true">

        <ImageView
            android:id="@+id/img_filemanager"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:layout_margin="3dp"
            android:padding="3dp"
            android:scaleType="centerCrop" />
    </com.droidlogic.tvlauncher.MyRelativeLayout>
    <com.droidlogic.tvlauncher.MyRelativeLayout
        android:id="@+id/layout_app"
        android:layout_width="87dp"
        android:layout_height="87dp"
        android:layout_marginLeft="817dp"
        android:layout_marginTop="288dp"
        android:descendantFocusability="beforeDescendants"
        android:focusable="true"
        android:focusableInTouchMode="true">

        <ImageView
            android:id="@+id/img_app"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:layout_margin="3dp"
            android:padding="3dp"
            android:scaleType="centerCrop" />
    </com.droidlogic.tvlauncher.MyRelativeLayout>
</RelativeLayout>
